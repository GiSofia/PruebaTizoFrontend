{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "data/db.json", "use": "@vercel/static-build" },
    { "src": "data/db-back.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/backend/(.*)", "dest": "/backend/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
